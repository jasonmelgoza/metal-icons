{
  "name": "metal-icons",
  "version": "0.1.0",
  "description": "A flexible React icon set",
  "main": "index.js",
  "author": "Jason Melgoza",
  "license": "MIT",
  "private": false,
  "scripts": {
    "optimize": "yarn optimize:line && yarn optimize:solid",
    "build": "yarn build:line && yarn build:solid",
    "optimize:line": "rimraf ./optimized/line & svgo -q -p 8 -f ./raw/line -o ./optimized/line",
    "optimize:solid": "rimraf ./optimized/solid & svgo -q -p 8 -f ./raw/line -o ./optimized/solid",
    "build:line": "svgr --out-dir src/line -- optimized/line",
    "build:solid": "svgr --out-dir src/solid -- optimized/solid"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@svgr/cli": "^8.0.1",
    "@svgr/core": "^8.0.0",
    "camelcase": "^7.0.1",
    "rimraf": "^5.0.1",
    "svgo": "^3.0.2",
    "terser": "^5.19.0"
  }
}
