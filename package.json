{
  "name": "metal-icons",
  "version": "0.1.0",
  "description": "A flexible React icon set",
  "main": "src/index.js",
  "author": "Jason Melgoza",
  "license": "MIT",
  "private": false,
  "files": ["src/"],
  "scripts": {
    "clean": "rimraf package *.tgz",
    "optimize": "yarn optimize:line:16 && yarn optimize:line:24 && yarn optimize:solid:16 && yarn optimize:solid:24",
    "build": "yarn build:line:16 && yarn build:line:24 && yarn build:solid:16 && yarn build:solid:24",
    "optimize:line:16": "rimraf ./optimized/16/outline && svgo -q -p 8 -f ./raw/16/outline -o ./optimized/16/outline",
    "optimize:line:24": "rimraf ./optimized/24/outline && svgo -q -p 8 -f ./raw/24/outline -o ./optimized/24/outline",
    "optimize:solid:16": "rimraf ./optimized/16/solid && svgo -q -p 8 -f ./raw/16/solid -o ./optimized/16/solid",
    "optimize:solid:24": "rimraf ./optimized/24/solid && svgo -q -p 8 -f ./raw/24/solid -o ./optimized/24/solid",
    "build:line:16": "svgr --out-dir src/16/outline -- optimized/16/outline",
    "build:line:24": "svgr --out-dir src/24/outline -- optimized/16/outline",
    "build:solid:16": "svgr --out-dir src/16/solid -- optimized/16/solid",
    "build:solid:24": "svgr --out-dir src/24/solid -- optimized/24/solid"
  },
  "devDependencies": {
    "@svgr/cli": "^8.0.1",
    "@svgr/core": "^8.0.0",
    "camelcase": "^7.0.1",
    "rimraf": "^5.0.1",
    "svgo": "^3.0.2"
  }
}
